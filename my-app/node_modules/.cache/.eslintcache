';\r\n              \r\n              if (ModifyType === 'food') {\r\n                fetchUrl = '/food_full_post';\r\n              } else if (ModifyType === 'rent') {\r\n                fetchUrl = '/rent_full_post';\r\n              } else if (ModifyType === 'changeClass') {\r\n                fetchUrl = '/course_full_post';\r\n              }\r\n              \r\n              if (fetchUrl) {\r\n                fetch(fetchUrl, {\r\n                  method: 'POST',\r\n                  headers: { 'Content-Type': 'application/json' },\r\n                  body: JSON.stringify(formData),\r\n                })\r\n                  .then((response) => response.json())\r\n                  .then((data) => {\r\n                    setData(data);\r\n                    console.log(data);\r\n                  })\r\n                  .catch((error) => {\r\n                    console.error('Error:', error);\r\n                  });\r\n              }\r\n            }\r\n          }, [data]);\r\n          \r\n          \r\n      \r\n        return (\r\n          <div className=\"PostArticle\">\r\n            <div className=\"PostArticle_bg\">\r\n              <Title>修改</Title>\r\n              <img className=\"post_elmo\" src={elmo} />\r\n              <img className=\"post_conversation\" src={conversation} />\r\n              <div className=\"articleFormPosition\">\r\n                <div className=\"articleForm\">\r\n                  <div className=\"inputFormPosition\">\r\n                    {isFoodShown && data && <ArticleModifyFoodInput newData={data}/>}\r\n                    {isRentShown && <ArticleModifyRentInput newData={data}/>}\r\n                    {isChangeClassShown && <ArticleChangeClassInput />}\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        );\r\n      }\r\n      \r\n\r\n    return (\r\n           \r\n                <Routes>\r\n                    <Route path=\"/\" element={<ModifyPost  />} />\r\n                </Routes>\r\n             \r\n    );\r\n}\r\n\r\nexport default ModifyPost;\r\n\r\n\r\n/* note\r\n-select要用onChange事件才可執行\r\n*/\r\n",{"ruleId":"197","severity":1,"message":"198","line":2,"column":27,"nodeType":"199","messageId":"200","endLine":2,"endColumn":33},{"ruleId":"197","severity":1,"message":"201","line":2,"column":34,"nodeType":"199","messageId":"200","endLine":2,"endColumn":38},{"ruleId":"197","severity":1,"message":"202","line":3,"column":10,"nodeType":"199","messageId":"200","endLine":3,"endColumn":16},{"ruleId":"197","severity":1,"message":"203","line":3,"column":18,"nodeType":"199","messageId":"200","endLine":3,"endColumn":23},{"ruleId":"197","severity":1,"message":"204","line":5,"column":9,"nodeType":"199","messageId":"200","endLine":5,"endColumn":13},{"ruleId":"197","severity":1,"message":"205","line":5,"column":15,"nodeType":"199","messageId":"200","endLine":5,"endColumn":21},{"ruleId":"197","severity":1,"message":"206","line":5,"column":23,"nodeType":"199","messageId":"200","endLine":5,"endColumn":28},{"ruleId":"197","severity":1,"message":"207","line":5,"column":30,"nodeType":"199","messageId":"200","endLine":5,"endColumn":44},{"ruleId":"197","severity":1,"message":"208","line":11,"column":12,"nodeType":"199","messageId":"200","endLine":11,"endColumn":20},{"ruleId":"197","severity":1,"message":"209","line":8,"column":91,"nodeType":"199","messageId":"200","endLine":8,"endColumn":112},{"ruleId":"197","severity":1,"message":"210","line":8,"column":132,"nodeType":"199","messageId":"200","endLine":8,"endColumn":152},{"ruleId":"197","severity":1,"message":"211","line":8,"column":223,"nodeType":"199","messageId":"200","endLine":8,"endColumn":240},{"ruleId":"197","severity":1,"message":"202","line":9,"column":10,"nodeType":"199","messageId":"200","endLine":9,"endColumn":16},{"ruleId":"197","severity":1,"message":"203","line":9,"column":18,"nodeType":"199","messageId":"200","endLine":9,"endColumn":23},{"ruleId":"